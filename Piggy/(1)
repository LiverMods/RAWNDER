coroutine.wrap(function()
    function SAD(object, wk)
        if object == "all" then
            local b = wk:GetDescendants()
            for i = 1, #b do
                b[i]:Destroy()
            end
        else
            local a = wk:GetDescendants()
            for i = 1, #a do
                if a[i].Name == object then
                    a[i]:Destroy()
                end
            end
        end
    end
    
    _G.GODMODE = false
    task.spawn(function()
        while task.wait() do
            if _G.GODMODE == true then
                local a = wk:getDescendants()
                for i = 1, #a do
                    if a[i]:FindFirstChild("Enemy") then
                        SAD("TouchInterest", a[i])
                    end
                end
            end
        end
    end)
end)()

