--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

coroutine.wrap(function()
    local __global = getfenv and getfenv(0) or _G
    local player = plrs.LocalPlayer
    local label = player:WaitForChild("PlayerGui"):WaitForChild("MainMenu"):WaitForChild("Timer")
    local RunService = RunService
    
    local lastText = label.Text
    local lastChange = tick()
    __global.Optimization = true
    
    RunService.Heartbeat:Connect(function()
        local currentText = label.Text

        if currentText ~= lastText then
            if currentText > lastText then
                __global.Optimization = false
            else
                __global.Optimization = true
            end
            lastText = currentText
            lastChange = tick()
        else
            if tick() - lastChange >= 2.5 then
                __global.Optimization = false
            else
                __global.Optimization = true
            end
        end
    end)
end)()

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local itemFrames = {}
coroutine.wrap(function()
    while true do
        local pasta = IsGame()
        if pasta then
            task.wait()

            for item, itemFrame in pairs(itemFrames) do
                if not item or not item:IsDescendantOf(pasta) then
                    itemFrame:Destroy()
                    itemFrames[item] = nil
                end
            end

            local items = {}
            for _, v in ipairs(pasta:GetDescendants()) do
                if (v.Name == "ItemPickupScript" or v.Name == "NewItemPickupScript" or v.Name == "Script") and
                   v.Parent and v.Parent:FindFirstChild("ClickDetector") then
                    table.insert(items, v.Parent)
                end
            end
            
            if _G.Traps then
                DestroyTraps()
            end
            
            if Optimization == true then
                for _, item in ipairs(items) do
                    if not item:IsA("BasePart") and not item:IsA("Model") then continue end
                    if item:IsA("BasePart") and (item.Transparency == 1 or not item:IsDescendantOf(workspace)) then continue end
                    if itemFrames[item] then continue end
                        
                    local U_i = Instance.new("UICorner")
                    U_i.CornerRadius = UDim.new(0.15, 0)
                    
                    local GradientColors = ColorSequence.new{
                        ColorSequenceKeypoint.new(0, Color3.fromRGB(85, 0, 0)),
                        ColorSequenceKeypoint.new(1, Color3.fromRGB(225, 0, 0))
                    }
                    
                    local ItemFrame = Instance.new("TextButton", ScrollingFrame)
                    ItemFrame.Name = randomString()
                    ItemFrame.BackgroundColor3 = Color3.new(0.10, 0, 0)
                    ItemFrame.BackgroundTransparency = 1
                    ItemFrame.Size = UDim2.new(0, 100, 0, 100)
                    ItemFrame.Text = "★"
                    U_i.Parent = ItemFrame
                    
                    local View = Instance.new("ViewportFrame", ItemFrame)
                    View.Name = randomString()
                    View.Size = UDim2.new(1, 0, 1, 0)
                    View.BackgroundTransparency = 0.4
                    View.BackgroundColor3 = Color3.fromRGB(85, 0, 0)
                    View.BorderSizePixel = 0
                    U_i:Clone().Parent = View
                    
                    local UiStroke = Instance.new("UIStroke")
                    UiStroke.Name = randomString()
                    UiStroke.Color = Color3.fromRGB(255, 255, 255)
                    UiStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
                    UiStroke.Parent = View
                    
                    local UiGradient = Instance.new("UIGradient")
                    UiGradient.Name = randomString()
                    UiGradient.Color = GradientColors
                    UiGradient.Rotation = 90
                    UiGradient.Parent = UiStroke
                    
                    local UiPadding = Instance.new("UIPadding")
                    UiPadding.PaddingBottom = UDim.new(0, 1)
                    UiPadding.PaddingLeft = UDim.new(0, 1)
                    UiPadding.PaddingRight = UDim.new(0, 1)
                    UiPadding.PaddingTop = UDim.new(0, 1)
                    UiPadding.Parent = ItemFrame
                    UiPadding.Name = randomString()
                    
                    local viewportclone
                    if item:IsA("BasePart") then
                        viewportclone = item:Clone()
                        viewportclone.Anchored = true
                    elseif item:IsA("Model") then
                        viewportclone = item:Clone()
                        for _, part in ipairs(viewportclone:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.Anchored = true
                            end
                        end
                    end
    
                    if viewportclone then
                        viewportclone.Name = randomString()
                        viewportclone.Parent = View
    
                        local cam = Instance.new("Camera")
                        cam.CameraType = Enum.CameraType.Fixed
                        cam.Parent = View
    
                        local primary = viewportclone:IsA("Model") and viewportclone.PrimaryPart or viewportclone
                        if not primary and viewportclone:IsA("Model") then
                            viewportclone.PrimaryPart = viewportclone:FindFirstChildWhichIsA("BasePart")
                            primary = viewportclone.PrimaryPart
                        end
    
                        if primary then
                            local objectPosition = primary.Position
                            local cameraPosition = objectPosition + Vector3.new(0, 3, 0)
                            cam.CFrame = CFrame.new(cameraPosition, objectPosition)
                            View.CurrentCamera = cam
                        end
                    else
                        ItemFrame:Destroy()
                        continue
                    end
    
                    for _, v in ipairs(pasta:GetDescendants()) do
                        if (v.Name == "ItemPickupScript" or v.Name == "NewItemPickupScript" or v.Name == "Script") and
                           v.Parent and v.Parent:FindFirstChild("ClickDetector") then
                            v.Parent.ClickDetector.MaxActivationDistance = 8999999488
                        end
                    end
                    
                    local itemNameValue = Instance.new("StringValue", ItemFrame)
                    itemNameValue.Name = "ItemName"
                    itemNameValue.Value = item.Name
                    itemFrames[item] = ItemFrame
    
                    ItemFrame.MouseButton1Up:Connect(function()
                        if item:FindFirstChild("ClickDetector") then
                            local player = plrs.LocalPlayer
                            local char = player.Character or player.CharacterAdded:Wait()
                            local root = char:WaitForChild("HumanoidRootPart")
                            local cpos = root.CFrame
                            local Sound = Instance.new("Sound", sound)
                            Sound.SoundId = "rbxassetid://3011722164"
                            Sound.PlaybackSpeed = 1.2
                            Sound.Volume = 1
                            
                            Sound:Play()
                            
                            repeat
                                pcall(function()
                                    for _ = 1, 2 do
                                        FastTeleport(item.CFrame)
                                        fireclickdetector(item:FindFirstChild("ClickDetector"))
                                    end
                                end)
                                task.wait()
                            until not (item and item.Parent and item:IsDescendantOf(pasta))
    
                            task.wait(0.05)
                            FastTeleport(cpos)
    
                            task.spawn(function()
                                local bolet = char:WaitForChild("Humanoid")
                                if bolet then
                                    bolet.Sit = true
                                    task.wait()
                                    bolet.Sit = false
                                end
                            end)
                        end
                    end)
                end
            elseif Optimization == false then
                for item, itemFrame in pairs(itemFrames) do
                    itemFrame:Destroy()
                end
                table.clear(itemFrames) 
            end
        end
        task.wait(delayTime)
    end
end)()
up(60)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    repeat RunService.Heartbeat:Wait()
    until roNub1 == 100
    local startTime = os.time()
    local Players = plrs
    local plr = Players.LocalPlayer
    
    local Wm = library2:Watermark("Ping: ")
    local FpsWm = Wm:AddWatermark("fps: " .. library2.fps)
    local HoraWm = Wm:AddWatermark("hora: --:--:--")
    local TempoWm = Wm:AddWatermark("online: 0s")
    
    coroutine.wrap(function()
    	while true do
    	    if roNub1 == 100 then
        		pcall(function()
        			FpsWm:Text("fps: " .. library2.fps)
        			Wm:Text("Ping: " .. math.round(plr:GetNetworkPing() * 1000) .. "")
        
        			local agora = os.date("*t")
        			local horaAtual = string.format("%02d:%02d:%02d", agora.hour, agora.min, agora.sec)
        			HoraWm:Text(Language.hora .. ": " .. horaAtual)
        
        			local diff = os.time() - startTime
        			local horas = math.floor(diff / 3600)
        			local minutos = math.floor((diff % 3600) / 60)
        			local segundos = diff % 60
        
        			local tempoStr = ""
        			if horas > 0 then
        				tempoStr = string.format("%dh %02dm %02ds", horas, minutos, segundos)
        			elseif minutos > 0 then
        				tempoStr = string.format("%dm %02ds", minutos, segundos)
        			else
        				tempoStr = string.format("%ds", segundos)
        			end
        
        			TempoWm:Text("online: " .. tempoStr)
        		end)
    		end
    		task.wait(.5)
    	end
    end)()
end)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    local backpack = CoreGui:WaitForChild("RobloxGui"):WaitForChild("Backpack")
    local inventory = backpack:WaitForChild("Inventory")
    
    local function stylizeButton(btn)
    	if btn:IsA("TextButton") and not btn:FindFirstChild("UICorner") then
    		btn.BackgroundTransparency = 0.4
    		btn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    
    		local uic = Instance.new("UICorner")
    		uic.CornerRadius = UDim.new(0, 8)
    		uic.Parent = btn
    	end
    end
    
    for _, v in pairs(inventory:GetDescendants()) do
    	stylizeButton(v)
    end
    
    inventory.DescendantAdded:Connect(function(desc)
    	task.wait()
    	stylizeButton(desc)
    end)
    
    local search = inventory:WaitForChild("Search")
    local border = Instance.new("Frame")
    border.Size = UDim2.new(1, 4, 1, 4)
    border.Position = UDim2.new(0, -2, 0, -2)
    border.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    border.BackgroundTransparency = 0.3
    border.ZIndex = search.ZIndex - 1
    border.Parent = search
    
    local borderCorner = Instance.new("UICorner")
    borderCorner.CornerRadius = UDim.new(0, 8)
    borderCorner.Parent = border
    
    local searchCorner = Instance.new("UICorner")
    searchCorner.CornerRadius = UDim.new(0, 8)
    searchCorner.Parent = search
end)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    coroutine.wrap(function()
        local myGui = CSI(function()
            local gui = Instance.new("ScreenGui")
            gui.Name = randomString()
            gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
            gui.DisplayOrder = -3
        
            return gui
        end)
        
        __global.gui = myGui
    
        local imageButton = Instance.new("ImageButton")
        imageButton.Name = randomString()
        imageButton.Size = UDim2.new(0.0325, 0, 0.076, 0)
        imageButton.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
        imageButton.BackgroundTransparency = 0.1
        imageButton.Image = "rbxassetid://18293640018"
        imageButton.Position = UDim2.new(0.20, 0, 0, 0)
        imageButton.Draggable = true
        
        local imageButtonBorder = Instance.new("UIStroke", imageButton)
        imageButtonBorder.Color = Color3.fromRGB(71, 71, 71)
        imageButtonBorder.Name = randomString()
        imageButton.Name = randomString()
        imageButtonBorder.Thickness = 1.5
        imageButton.Parent = gui
        
        booletan = false
        local originalPos = Frame.Position
        local lastClick = 0
        
        imageButton.MouseButton1Click:Connect(function()
            if tick() - lastClick < 0.3 then return end
            lastClick = tick()
            
            task.spawn(function()
                local Sound = Instance.new("Sound", sound)
                Sound.SoundId = "rbxassetid://4601634016"
                Sound:Play()
                
                booletan = not booletan
                imageButton.Image = booletan and "rbxassetid://18293634731" or "rbxassetid://18293640018"
                
                for i = 1, 5 do
                    task.wait()
                    gui.DisplayOrder = -3
                    PiggyGui.DisplayOrder = -3
                end 
                
                if booletan then
                    originalPos = Frame.Position
                    tw(Frame, UDim2.new(originalPos.X.Scale, originalPos.X.Offset, 1.8, 0), 0.58)
                else
                    tw(Frame, originalPos, 0.48)
                end
            end)
        end)
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0.2, 0.1)
        corner.Parent = imageButton
        corner.Name = randomString()
        
        coroutine.wrap(function()
            task.wait(2)
            while task.wait() do
                gui.RobloxLocked = true
                gui.ResetOnSpawn = false
                PiggyGui.ResetOnSpawn = false
                PiggyGui.Enabled = true
                gui.Enabled = true
                ScrollingFrame.Active = true
                ScrollingFrame.Visible = true
                ScrollingFrame.ScrollingEnabled = true
                imageButton.Visible = true
            end
        end)()
    end)()
end)

coroutine.wrap(function()
    local ui = __global.gui
    while true do
        if roNub1 == 100 then
            pcall(function()
                encryptNames(PiggyGui)
                encryptNames(ui)
            end) task.wait(10)
        else
            task.wait()
        end
    end
end)()

