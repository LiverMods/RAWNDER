--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local itemButtons = {}
local textoAnterior = nil
local tempoCongelado = 0
local tempoLimite = 3

coroutine.wrap(function()
	while true do
		local player = game:GetService("Players").LocalPlayer
		local gui = player:FindFirstChild("PlayerGui")
		local menu = gui and gui:FindFirstChild("MainMenu")
		local timerLabel = menu and menu:FindFirstChild("Timer")
		local timerVisivel = timerLabel and timerLabel:IsA("TextLabel") and timerLabel.Visible

		local deveResetar = false

		if not menu or not timerLabel or not timerVisivel then
			deveResetar = true
		else
			local tempoAtual = timerLabel.Text
			if tempoAtual == textoAnterior then
				tempoCongelado += task.wait()
				if tempoCongelado >= tempoLimite then
					deveResetar = true
				end
			else
				textoAnterior = tempoAtual
				tempoCongelado = 0
			end
		end

		if deveResetar then
			for _, button in pairs(itemButtons) do
				button:Destroy()
			end
			itemButtons = {}
			textoAnterior = nil
			tempoCongelado = 0
			task.wait(1)
			continue
		end

		local pasta = IsGame()
		if not pasta then
			task.wait()
			continue
		end

		local descendants = pasta:GetDescendants()
		local currentItems = {}

		for _, obj in ipairs(descendants) do
			if obj.Name == "ItemPickupScript" or obj.Name == "NewItemPickupScript" or (obj.Name == "Script" and obj.Parent:FindFirstChild("ClickDetector")) then
				table.insert(currentItems, obj.Parent)
			end
		end

		table.sort(currentItems, function(a, b)
			return a.Name < b.Name
		end)

		local currentNames = {}
		for _, item in ipairs(currentItems) do
			currentNames[item.Name] = true
		end

		for name, button in pairs(itemButtons) do
			if not currentNames[name] then
				button:Destroy()
				itemButtons[name] = nil
			end
		end

		for _, item in ipairs(currentItems) do
			if not itemButtons[item.Name] then
				local U_i = Instance.new("UICorner")
				U_i.CornerRadius = UDim.new(0.15, 0)
				U_i.Name = randomString()

				local ItemFrame = Instance.new("TextButton", ScrollingFrame)
				ItemFrame.Name = randomString()
				ItemFrame.BackgroundColor3 = Color3.new(0.10, 0, 0)
				ItemFrame.BackgroundTransparency = 1
				ItemFrame.Size = UDim2.new(0, 100, 0, 100)
				ItemFrame.Text = "★"
				U_i.Parent = ItemFrame

				local View = Instance.new("ViewportFrame", ItemFrame)
				View.Name = randomString()
				View.Size = UDim2.new(1, 0, 1, 0)
				View.BackgroundTransparency = 0.9
				View.BorderSizePixel = 0.3
				U_i.Parent = View

				local viewportclone = item:Clone()
				viewportclone.Parent = View
				viewportclone.Name = randomString()

				local cam = Instance.new("Camera", viewportclone)
				cam.CameraType = Enum.CameraType.Fixed

				local objectPosition = item.Position
				local cameraPosition = objectPosition + Vector3.new(0, 3, 0)
				cam.CoordinateFrame = CFrame.new(cameraPosition, objectPosition)
				View.CurrentCamera = cam

				local debounce = false
				ItemFrame.MouseButton1Down:Connect(function()
					if debounce then return end
					debounce = true

					if item:FindFirstChild("ClickDetector") then
						local character = player.Character
						if character and character.PrimaryPart then
							local cpos = character:GetPrimaryPartCFrame()
							MoveTo(item.CFrame)
							task.wait(0.2)
							for _ = 1, 20 do
								pcall(function()
									for _ = 1, 2 do
										fireclickdetector(item.ClickDetector)
									end
								end)
							end
							task.wait(0.2)
							for _ = 1, 5 do
								MoveTo(cpos)
							end
						end
					end

					debounce = false
				end)

				itemButtons[item.Name] = ItemFrame
			end
		end

		local index = 0
		for _, item in ipairs(currentItems) do
			local button = itemButtons[item.Name]
			if button then
				button.Position = UDim2.new(0, 0, 0, index * 110)
				index += 1
			end
		end

		task.wait()
	end
end)()
up(60)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    repeat RunService.Heartbeat:Wait()
    until roNub1 == 100
    local startTime = os.time()
    local Players = game:GetService("Players")
    local plr = Players.LocalPlayer
    
    local Wm = library2:Watermark("Ping: ")
    local FpsWm = Wm:AddWatermark("fps: " .. library2.fps)
    local HoraWm = Wm:AddWatermark("hora: --:--:--")
    local TempoWm = Wm:AddWatermark("online: 0s")
    
    coroutine.wrap(function()
    	while true do
    	    if roNub1 == 100 then
        		pcall(function()
        			FpsWm:Text("fps: " .. library2.fps)
        			Wm:Text("Ping: " .. math.round(plr:GetNetworkPing() * 1000) .. "")
        
        			local agora = os.date("*t")
        			local horaAtual = string.format("%02d:%02d:%02d", agora.hour, agora.min, agora.sec)
        			HoraWm:Text(Language.hora .. ": " .. horaAtual)
        
        			local diff = os.time() - startTime
        			local horas = math.floor(diff / 3600)
        			local minutos = math.floor((diff % 3600) / 60)
        			local segundos = diff % 60
        
        			local tempoStr = ""
        			if horas > 0 then
        				tempoStr = string.format("%dh %02dm %02ds", horas, minutos, segundos)
        			elseif minutos > 0 then
        				tempoStr = string.format("%dm %02ds", minutos, segundos)
        			else
        				tempoStr = string.format("%ds", segundos)
        			end
        
        			TempoWm:Text("online: " .. tempoStr)
        		end)
    		end
    		task.wait(.5)
    	end
    end)()
    
    
    pcall(function()
        encryptNames(game:GetService("CoreGui").watermark)
    end)
end)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    local CoreGui = game:GetService("CoreGui")
    local backpack = CoreGui:WaitForChild("RobloxGui"):WaitForChild("Backpack")
    local inventory = backpack:WaitForChild("Inventory")
    
    local function stylizeButton(btn)
    	if btn:IsA("TextButton") and not btn:FindFirstChild("UICorner") then
    		btn.BackgroundTransparency = 0.4
    		btn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    
    		local uic = Instance.new("UICorner")
    		uic.CornerRadius = UDim.new(0, 8)
    		uic.Parent = btn
    	end
    end
    
    for _, v in pairs(inventory:GetDescendants()) do
    	stylizeButton(v)
    end
    
    inventory.DescendantAdded:Connect(function(desc)
    	task.wait()
    	stylizeButton(desc)
    end)
    
    local search = inventory:WaitForChild("Search")
    local border = Instance.new("Frame")
    border.Size = UDim2.new(1, 4, 1, 4)
    border.Position = UDim2.new(0, -2, 0, -2)
    border.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
    border.BackgroundTransparency = 0.3
    border.ZIndex = search.ZIndex - 1
    border.Parent = search
    
    local borderCorner = Instance.new("UICorner")
    borderCorner.CornerRadius = UDim.new(0, 8)
    borderCorner.Parent = border
    
    local searchCorner = Instance.new("UICorner")
    searchCorner.CornerRadius = UDim.new(0, 8)
    searchCorner.Parent = search
end)

