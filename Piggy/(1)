local function songIntro()
    task.spawn(function()
        task.wait() local Sound = Instance.new("Sound",sound) Sound.SoundId = "rbxassetid://488265234" Sound.Volume = 2.6 Sound:Play()
    end)
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function getFullPath(obj)
	local path = obj.Name
	local parent = obj.Parent
	while parent and parent ~= game do
		path = parent.Name .. "." .. path
		parent = parent.Parent
	end
	return "workspace." .. path
end

local function isCharacterLike(obj)
	local hasHumanoid = false
	local hasRootPart = false

	for _, child in ipairs(obj:GetDescendants()) do
		if child:IsA("Humanoid") then
			hasHumanoid = true
		elseif child.Name == "HumanoidRootPart" and child:IsA("BasePart") then
			hasRootPart = true
		end
		if hasHumanoid and hasRootPart then
			return true
		end
	end

	return false
end

local function findAllSafeTraps()
	local traps = {}
	for _, obj in ipairs(workspace:GetDescendants()) do
		if string.lower(obj.Name):find("trap") and not isCharacterLike(obj) then
			table.insert(traps, obj)
		end
	end
	return traps
end

function SAD2(objectName, parentObj)
	local descendants = parentObj:GetDescendants()
	for _, d in ipairs(descendants) do
		if objectName == "all" or d.ClassName == objectName then
			d:Destroy()
		end
	end
end

local function DestroyTraps()
	local traps = findAllSafeTraps()
	if #traps == 0 then
		return
	end

	for _, trapObj in ipairs(traps) do
		pcall(function()
			SAD2("TouchTransmitter", trapObj)
		end)
	end
end

local function __creatUi()
    if get_hidden_gui or gethui then
        local hiddenUI = get_hidden_gui or gethui
        local PiggyGui = Instance.new("ScreenGui")
        PiggyGui.Name = randomString()
        PiggyGui.ResetOnSpawn = false
        PiggyGui.DisplayOrder = 9e9
        PiggyGui.Enabled = true
        PiggyGui.Parent = hiddenUI()
        return PiggyGui
    elseif (not is_sirhurt_closure) and (syn and syn.protect_gui) then
        local PiggyGui = Instance.new("ScreenGui")
        PiggyGui.Name = randomString()
        PiggyGui.ResetOnSpawn = false
        PiggyGui.DisplayOrder = 9e9
        PiggyGui.Enabled = true
        
        syn.protect_gui(PiggyGui)
        
        PiggyGui.Parent = CoreGui
        return PiggyGui
    else
        local PiggyGui = Instance.new("ScreenGui")
        PiggyGui.Name = randomString()
        PiggyGui.ResetOnSpawn = false
        PiggyGui.DisplayOrder = 9e9
        PiggyGui.Enabled = true
        PiggyGui.Parent = CoreGui
        return PiggyGui
    end
end

local function CSI(buildFunction)
    assert(typeof(buildFunction) == "function")

    local gui = buildFunction()
    assert(typeof(gui) == "Instance")
    assert(gui:IsA("ScreenGui"))

    local success, err = pcall(function()
        if get_hidden_gui or gethui then
            local hidden = get_hidden_gui or gethui
            gui.Parent = hidden()
        elseif (not is_sirhurt_closure) and syn and syn.protect_gui then
            syn.protect_gui(gui)
            gui.Parent = CoreGui
        else
            gui.Parent = CoreGui
        end
    end)

    if not success then
        warn("[CreateSafeGui]: Failed to apply protection/parent: " .. tostring(err))
        return nil
    end

    return gui
end

