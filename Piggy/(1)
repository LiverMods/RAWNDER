            loadstring(game:HttpGet("https://gist.githubusercontent.com/LiverMods/e078befcb255a18acf13f16177a5e4e1/raw/921145e555d53c43cd85176d89baf79ff4ccef05/inventory"))();
        end)
    end)
    
    task.spawn(function()
        task.wait(random(15, 30))
        AkaliNotif_(Language.NewVersion, Language.AdvancedFeatures, 7.8)
    end)
end)

