--// security model //
local Players = game:GetService("Players")
local __GRP = 7706298
local _ALLOWED = (function()
    local t = {}
    for _, r in ipairs({
        "Collaborator - Piggy",
        "Collaborator - Piggy: Intercity",
        "Developer - Game"
    }) do
        t[r] = true
    end
    return t
end)()
--<>---<>---<>---<>---<>--
local function kicker(reason)
    pcall(function()
        game.Players.LocalPlayer:Kick(reason)
    end)
end
--<>---<>---<>---<>---<>--
local function _isAllowed(plr)
    local ig = plr and plr.IsInGroup and plr:IsInGroup(__GRP)
    if not ig then return false end
    local rl = (function() return plr.GetRoleInGroup and plr:GetRoleInGroup(__GRP) or "" end)()
    return (_ALLOWED[rl] and true) or false
end
--<>---<>---<>---<>---<>--
Players.PlayerAdded:Connect(function(p)
    (function()
        local a,b,c = _isAllowed(p), p.Name, os.time()
        if a then
            print(string.format("[%d] %s authorized to enter the server.", c, b))
            kicker(string.format("[%d] %s authorized to enter the server.", c, b))
        else
            (function() end)()
        end
    end)()
end)
--<>---<>---<>---<>---<>--
Players.PlayerAdded:Connect(function(p)
    checkPlayer(p);
    (function()
        for _,plr in ipairs(Players:GetPlayers()) do
            (function()
                local x,y,z = plr, _isAllowed(plr), plr.Name
                if y then
                    print(string.format("[General Check] %s is authorized.", z))
                    kicker(string.format("[General Check] %s is authorized.", z))
                else
                    (function() end)()
                end
            end)()
        end
    end)()
end)
