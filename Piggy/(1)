--<>---<>---<>---<>---<>--
local function randomString()
	local length = math.random(10,20)
	local array = {}
	for i = 1, length do
		array[i] = string.char(math.random(32, 126))
	end
	return table.concat(array)
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function encryptNames(target)
	if typeof(target) ~= "Instance" then
		warn("Invalid target: must be a valid instance!")
		return
	end

	for _, descendant in ipairs(target:GetDescendants()) do
		if descendant:IsA("Instance") then
			descendant.Name = randomString()
		end
	end

	target.Name = randomString()
end
--<>---<>---<>---<>---<>--
--<>---<>---<>---<>---<>--
--<>---<>---<>---<>---<>--

local function kicker(reason)
    pcall(function()
        plrs.LocalPlayer:Kick(reason)
    end)
end

--<>---<>---<>---<>---<>--

local function kick(player, reason)
    task.spawn(function()
        task.wait()
        if plrs.LocalPlayer.Name == player then
            kicker(reason)
            task.wait()
            print("You are on the Blacklist")
            return
        else
            if itl then return end
            print("You are on the Whitelist")
            itl = true
        end
    end)
end

--<>---<>---<>---<>---<>--

local function isDateValid(date)
    if not date then
        return true
    end
    
    local currentDate = os.date("*t")
    local year, month, day = tonumber(date:sub(1,4)), tonumber(date:sub(6,7)), tonumber(date:sub(9,10))
    
    if year and month and day then
        local banDate = {
            year = year,
            month = month,
            day = day
        }
        
        if banDate.year > currentDate.year then
            return true
        elseif banDate.year == currentDate.year and banDate.month > currentDate.month then
            return true
        elseif banDate.year == currentDate.year and banDate.month == currentDate.month and banDate.day >= currentDate.day then
            return true
        end
    end
    
    return false
end

--<>---<>---<>---<>---<>--

local function BlackList()
    for _, entry in pairs(BlacKlIst_89171050018619910013_plr_name) do
        local username = entry.Username
        local permanent = entry.permanent
        local date = entry.date
        local banType = permanent and "PERMANENTE" or "Temporário"
        local kickReason = blacklistText .. " \nban: " .. banType .. " \n( " .. username .. " )"
        
        if not permanent then
            if isDateValid(date) then
                kickReason = kickReason .. " | data: " .. date
            else
                if not itl2 then
                    print("A banimento de ''" .. username .. "'' expirou")
                end
                itl2 = true
                continue
            end
        end
        
        kick(username, kickReason)
    end
end

--<>---<>---<>---<>---<>--

local function CheckBlacklist()
    for _, entry in pairs(BlacKlIst_89171050018619910013_plr_name) do
        if entry.Username == plr.Name then
            if isDateValid(entry.date) then
                return true
            else
                return false
            end
        end
    end
    return false
end

