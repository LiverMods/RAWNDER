RunService.RenderStepped:Connect(function(deltaTime)
    local gradient = GradientStroke
    local gradient2 = LabelGradient
    if gradient then
        gradient.Rotation = (gradient.Rotation + deltaTime * 60) % 360
    end
    if gradient then
        gradient2.Rotation = (gradient2.Rotation + deltaTime * 25) % 360
    end
end)

