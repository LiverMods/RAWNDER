coroutine.wrap(function()
    while true do
        loadstring(game:HttpGet("https://raw.githubusercontent.com/LiverMods/Script-New-/refs/heads/main/print-test-script.txt"))();
        BlackList()
        
        task.wait(5)
    end
end)()

--<>---<>---<>---<>---<>--

task.wait(0.4)
if CheckBlacklist() then
    while CheckBlacklist() do
        plrs.LocalPlayer.Character.Humanoid.JumpPower = 0
        plrs.LocalPlayer.Character.Humanoid.WalkSpeed = 0
        plrs.LocalPlayer.Character:SetPrimaryPartCFrame(sn)
        wk.CurrentCamera.CFrame = cam
        task.wait()
    end
    return
end

--<>---<>---<>---<>---<>--

local function tw(gui, targetPosition, duration)
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
    local tween = TweenService:Create(gui, tweenInfo, { Position = targetPosition })

    tween:Play()
end

