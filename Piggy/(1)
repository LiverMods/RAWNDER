up(10)

local function findFolder(scriptName)
    local scriptObject = nil
    
    while not scriptObject do
        for _, obj in ipairs(workspace:GetDescendants()) do
            if obj.Name == scriptName then
                scriptObject = obj
                break
            end
        end
        task.wait(0.5)
    end
    
    local parent = scriptObject.Parent
    while parent and parent.Parent ~= workspace do
        parent = parent.Parent
    end
    return parent
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function GetPart(Target)
    if Target and Target.Character then
        return Target.Character:FindFirstChild("HumanoidRootPart") or Target.Character:FindFirstChild("Head")
    end
    return nil
end


--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function AkaliNotif_(Title, Description, time)
    local Notify = AkaliNotif.Notify;
    Notify({
        Description = Description,
        Title = Title,
        Duration = time
    });
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

task.spawn(function()
    local placeId = game.PlaceId
    local doors = {}
    local CTS
    
    local function HCWCN(LM, requiredClassNames)
    	local found = {}
    	for _, className in ipairs(requiredClassNames) do
    		found[className] = false
    	end
    
    	for _, descendant in ipairs(LM:GetDescendants()) do
    		if found[descendant.ClassName] == false then
    			found[descendant.ClassName] = true
    		end
    	end
    
    	for _, className in ipairs(requiredClassNames) do
    		if not found[className] then
    			return false
    		end
    	end
    
    	return true
    end
    
    if placeId == 4623386862 then
    	CTS = { "Sound", "ClickDetector", "BoolValue", "Script" }
    elseif placeId == 5661005779 or placeId == 5247067896 then
    	CTS = { "RemoteEvent", "WeldConstraint", "ClickDetector" }
    end
    
    if CTS then
    	for _, v in ipairs(workspace:GetDescendants()) do
    		if v:IsA("Model") or v:IsA("Part") then
    			if HCWCN(v, CTS) then
    				table.insert(doors, v)
    			end
    		end
    	end
    end
    
    for _, door in ipairs(doors) do
        for _, v in ipairs(door:GetDescendants()) do
            if v:IsA("ClickDetector") then
            	v.MaxActivationDistance = 8999999488
            end
        end
    end
end)

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

function SAD(object, w1k)
    if object == "all" then
        local b = w1k:GetDescendants()
        for i = 1, #b do
            b[i]:Destroy()
        end
    else
        local a = w1k:GetDescendants()
        for i = 1, #a do
            if a[i].Name == object then
                a[i]:Destroy()
            end
        end
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

function random(min, max)
    if min and max then
        return math.random(min, max)
    elseif min then
        return math.random(0.1, min)
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

function GetPosition(Player)
    plr.Character:WaitForChild("HumanoidRootPart")
    if Player then
        return GetPart(Player).CFrame
    else
        return GetPart(plr).CFrame
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function swait()
    RunService.Stepped:Wait()
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function GetCameraPosition()
    return wk.CurrentCamera.CFrame
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function Loop(Times, calling)
    for i = 1, tonumber(Times) do
        calling()
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function ConvertPosition(Position)
    if typeof(Position):lower() == "vector3" then
        return CFrame.new(Position)
    else
        return Position
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function UnSit()
    plr.Character:WaitForChild("Humanoid").Sit = false
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function MoveTo(Cframe)
    Cframe = ConvertPosition(Cframe)
    local Amount = 5

    for i = 1, Amount do
        UnSit()
        plr.Character:PivotTo(Cframe)
        swait()
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function FastTeleport(targetPos)
	local character = plr.Character
	if not character or not character:FindFirstChild("HumanoidRootPart") then return end

	local hrp = character.HumanoidRootPart
	local cframe = ConvertPosition(targetPos)

	local humanoid = character:FindFirstChild("Humanoid")
	if humanoid then
		humanoid.PlatformStand = true
	end

	for i = 1, 5 do
		UnSit()
		character:PivotTo(cframe)
		swait()
	end

	if humanoid then
		humanoid.PlatformStand = false
	end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function IsGame()
    if game.PlaceId == 4623386862 then
        return findFolder("ItemPickupScript")
    elseif game.PlaceId == 5661005779 or game.PlaceId == 5247067896 then
        return findFolder("NewItemPickupScript")
    end
end

