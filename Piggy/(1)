            local teleportService = game:GetService("TeleportService")
            
            AddSound("error")

            function Basic.OnInvoke(state)
                if state == 'join Piggy' then
                    pcall(function()
                        task.spawn(function()
                            if not hookmetamethod then 
                        		return print('Explora√ß√£o incompat√≠vel. Sua explora√ß√£o n√£o suporta este comando (hookmetamethod ausente)')
                        	end
                        	
