local function encontrarPasta(scriptName)
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj.Name == scriptName then
            local parent = obj.Parent
            while parent and parent.Parent ~= workspace do
                parent = parent.Parent
            end
            return parent --// Retorna a pasta que é parent da workspace //
        end
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function IsGame()
    if game.PlaceId == 4623386862 then
        return encontrarPasta("ItemPickupScript")
    elseif game.PlaceId == 5661005779 or game.PlaceId == 5247067896 then
        return encontrarPasta("NewItemPickupScript")
    end
end

--<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>----<>--

local function randomString()
	local length = math.random(10,20)
	local array = {}
	for i = 1, length do
		array[i] = string.char(math.random(32, 126))
	end
	return table.concat(array)
end

