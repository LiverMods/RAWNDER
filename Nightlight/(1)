local bs = game.PlaceId
local normal = 15507705710
local hard = 15628391893
local dat, mon, yea = 04, 02, 2024

local function check(id, _namecall)
    local index = table.find(id, game.PlaceId)
    if index then
        _namecall[index]()
    end
end

check({normal, hard}, {
    function()
        loadstring(game:HttpGet("https://abre.ai/lm-nightlight"))();
    end,
    function()
        loadstring(game:HttpGet("https://abre.ai/lm-nightlight"))();
    end
})
