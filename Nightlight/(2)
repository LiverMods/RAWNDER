        end)()
        jumper = mmRawnder
        coroutine.wrap(function()
            pcall(function()
                while jumper do
                    if 56 ~= charr.Humanoid.JumpPower then
                        charr.Humanoid.JumpPower = 56
                    end
                    task.wait()
                end
            end)
        end)()
    else
        speeder = false
        jumper = false
        task.wait()
        charr:FindFirstChildOfClass('Humanoid').UseJumpPower = b
        charr.Humanoid.WalkSpeed = a
    end
end)

