end)

plrS_:NewToggle("Auto SET", "speed!!", function(mmRawnder)
    speeder = mmRawnder
    coroutine.wrap(function()
        pcall(function()
            while speeder do
                if velocidade ~= charr.Humanoid.WalkSpeed then
                    charr.Humanoid.WalkSpeed = velocidade
                end
                task.wait()
            end
        end)
    end)()
end)

