local function SAD(object, wk)
    if object == "all" then
        local b = wk:getDescendants()
        for i = 1, #b do
            b[i]:Destroy()
        end
    else
        local a = wk:getDescendants()
        for i = 1, #a do
            if a[i].Name == object then
                a[i].Parent.CanCollide = false
                a[i]:Destroy()
            end
        end
    end
end


