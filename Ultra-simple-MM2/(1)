        Name = "Redz Hub | Community | V5";
        Description = "Interface made by real_redz";
        Logo = "rbxassetid://15298567397";
        Invite = "https://discord.gg/7aR7kNVt4g"
    })
    while task.wait(0.45) do
        local pl_rs = game.Players:GetPlayers()
        inf:Set("Players: " .. #pl_rs .. "/" .. game.Players.MaxPlayers, " ")
        inf6:Set("Last update: " ..dat.. "/" ..mon.. "/" ..yea, " ")
        inf7:Set("FPS: " .. library.fps, " ")
        inf8:Set("Ping: " .. math.round(plr:GetNetworkPing() * 1000) .. ".ms", "This measure is known as latency, sometimes also called lag, between the computer and its server.")
    end
end)()

