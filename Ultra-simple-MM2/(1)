Tab1:AddButton({"HitBox", function()
    task.spawn(function()
        local Dialog = Window:Dialog({
            Title = "He is sure?",
            Text = "You won't be able to deactivate it!",
            Options = {
                {"✅", function()
                    addHBx()
                end},
                {"❌", function()
                    return
                end}
            }
        })
    end)
    
    function addHBx()
        function getPlayersName()
            for _, v in pairs(game:GetChildren()) do
                if v.ClassName == "Players" then
                    return v.Name
                end
            end
        end
        
        local players = getPlayersName()
        local plr = game[players].LocalPlayer
        
        coroutine.resume(coroutine.create(function()
            while wait(1) do
                coroutine.resume(coroutine.create(function()
                    for _, v in pairs(game[players]:GetPlayers()) do
                        if v.Name ~= plr.Name and v.Character then
                            local lowerTorso = v.Character:FindFirstChild("LowerTorso")
                            local torso = v.Character:FindFirstChild("Torso")
        
                            if lowerTorso then
                                lowerTorso.Transparency = 0.85
                                lowerTorso.CanCollide = false
                                lowerTorso.Material = "Neon"
                                lowerTorso.Size = Vector3.new(5.5, 4.6, 5.5)
                            elseif torso then
                                torso.Transparency = 0.85
                                torso.CanCollide = false
                                torso.Material = "Neon"
                                torso.Size = Vector3.new(5.5, 4.6, 5.5)
                            end
                        end
                    end
                end))
            end
        end))
    end
end})

